   "source": [
    "import sys\n",
    "\n",
    "cmd_num = int(sys.stdin.readline().rstrip())\n",
    "result = 1\n",
    "\n",
    "if cmd_num == 0 or cmd_num == 1:\n",
    "    print(1)\n",
    "\n",
    "else:\n",
    "    for i in range(cmd_num, 1, -1):\n",
    "        result *= i\n",
    "    \n",
    "    print(result)"
